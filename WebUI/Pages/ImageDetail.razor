@inject Bulk_Thumbnail_Creator.Services.PicDataService PicDataService;
@using Bulk_Thumbnail_Creator.PictureObjects;

<PictureDataBox ImageURL="@ImageUrl" PicData="@CurrentPagePictureData" />

<VarietyDisplay ParentPictureProp="CurrentPagePictureData" VarietyUrls="VarietyUrls"></VarietyDisplay>

@code
{
    [Parameter]
    public List<string> VarietyUrls { get; set; }

    [Parameter]
    public PictureData CurrentPagePictureData { get; set; }

    [Parameter]
    public string ImageUrl { get; set; }

}