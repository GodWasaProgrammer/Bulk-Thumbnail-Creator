@inject Bulk_Thumbnail_Creator.PictureObjects.PictureDataService PictureDataService;
@page "/imagedetail/{imageUrl}"

<div class="container">
    <div class="row">
        <div class="col-md-6">
            <img src="@ImageUrl" style="max-width: 100%;" />
        </div>
        <div class="col-md-6">
            <PictureDataBox PictureDataProp="@CurrentPagePictureData" />
        </div>
    </div>
</div>




